<body>
<div class = "container">
    <h2> <%= title %> </h2>
    <table class = "table table-dark table-striped">
        <thead>
            <tr>
                <th> TIP_UST </th>
                <th> VU_IDENTIFIKATOR </th> 
                <th> ZAP_REDNI_BROJ </th>
                <th> ZAP_PREZIME </th>
                <th> ZAP_SREDNJE_SLOVO </th>
                <th> ZAP_IME </th>
                <th> ZAP_FOTOGRAFIJA </th>
                <th> ZAP_JMBG </th>
                <th> RCP_REDNI_BROJ </th>
            </tr>
        </thead>
        <tbody> 
            <% employees.forEach(function(employees) { %>
                <tr>
                    <td> <%= employees.TIP_US %> </td>
                    <td> <%= employees.VU_IDENTIFIKATOR %> </td>
                    <td> <%= employees.ZAP_REDNI_BROJ %> </td>
                    <td> <%= employees.ZAP_PREZIME %> </td>
                    <td> <%= employees.ZAP_SREDNJE_SLOVO %> </td>
                    <td> <%= employees.ZAP_IME %> </td>
                    <td> <%= employees.ZAP_FOTOGRAFIJA %> </td>
                    <td> <%= employees.ZAP_JMBG %> </td>
                    <td> <%= employees.RCP_REDNI_BROJ %> </td>
                    <td> 
                        <button class = "btn btn-sm btn-primary"> Edit </button>
                        <button class = "btn btn-sm btn-danger"> Delete </button>
                    </td> 
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
</body>
</html>